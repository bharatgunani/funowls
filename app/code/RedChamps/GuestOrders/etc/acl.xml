<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Sales::sales">
                    <resource id="RedChamps_GuestOrders::menu" title="Guest Orders To Customer" translate="title" sortOrder="10">
                        <resource id="RedChamps_GuestOrders::processed" title="Processed Orders" translate="title" sortOrder="10" />
                        <resource id="RedChamps_GuestOrders::followed" title="Followed up Orders" translate="title" sortOrder="20" />
                        <resource id="RedChamps_GuestOrders::pending" title="Pending Guest Orders" translate="title" sortOrder="30" />
                        <resource id="RedChamps_GuestOrders::settings" title="Settings" translate="title" sortOrder="40" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="RedChamps_GuestOrders::config" title="Guest Orders Setting" translate="title" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
